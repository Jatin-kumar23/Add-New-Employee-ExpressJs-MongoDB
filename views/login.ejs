<%- include ('header') %> 
<div id="container">
    <h1 class="center h-primary">Login </h1>
 </div>
<form id="loginform" action="/login" method="POST">
    <input class="form loginform" type="text" name="username" placeholder="username" required>
    <input class="form loginform" type="password" name="password" placeholder="password" required>
    <div id="submit-btn" class="center ">
        <button class="btn ">Login</button>
     </div>
</form>

